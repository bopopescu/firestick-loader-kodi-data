<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="plugin.video.oldtimeradio"
	name="Old Time Radio on YouTube"
	version="1.0.0"
	provider-name="coldkeys">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
    <import addon="script.module.addon.common" version="2.0.0"/>
    <import addon="plugin.video.youtube" version="5.0.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>video audio</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Old Time Radio</summary>
    <description lang="en">Mammoth selection of Old Time Radio shows on YouTube.</description>
    <platform>all</platform>
    <forum>n/a</forum>
    <website>n/a</website>
  </extension>
</addon>
